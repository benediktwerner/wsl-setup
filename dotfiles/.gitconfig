[user]
        email = 1benediktwerner@gmail.com
        name = Benedikt Werner
        signingkey = 1DBFF0F8E9E121EB
[core]
        autocrlf = false
        safecrlf = false
        excludesfile = ~/.gitignore
        eol = lf
        pager = delta
[init]
        defaultBranch = "master"
[delta]
        line-numbers = true
        minus-style = "syntax auto"
        minus-emph-style = "syntax auto"
        plus-style = "syntax 22"
        plus-emph-style = "syntax 34"
        line-numbers-minus-style = "9"
        line-numbers-zero-style = "8"
        line-numbers-plus-style = "10"
[alias]
        c = commit
        s = status
        f = fetch
        fup = fetch upstream
        rup = reset --hard @{upstream}
        pushh = push origin HEAD
        last = log -1 HEAD
        graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
[push]
        default = simple
[pull]
        ff = only
[help]
        autoCorrect = 1
[url "git@github.com:"]
        insteadOf = gh:
[diff]
        colorMoved = default
        algorithm = patience
        compactionHeuristic = true
[commit]
        gpgsign = true
[hub]
        protocol = ssh
